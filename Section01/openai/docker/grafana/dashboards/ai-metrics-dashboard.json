{
  "uid": "spring-ai-basic",
  "title": "Spring AI â€“ Basic Metrics (Working)",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Total AI Requests",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "__PROM_DS__" },
      "targets": [
        {
          "expr": "sum(gen_ai_client_operation_seconds_count)",
          "refId": "A"
        }
      ]
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Requests / Second",
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "__PROM_DS__" },
      "targets": [
        {
          "expr": "sum(rate(gen_ai_client_operation_seconds_count[1m]))",
          "refId": "A"
        }
      ]
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Average Latency (ms)",
      "gridPos": { "x": 0, "y": 4, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "__PROM_DS__" },
      "targets": [
        {
          "expr": "(rate(gen_ai_client_operation_seconds_sum[1m]) / rate(gen_ai_client_operation_seconds_count[1m])) * 1000",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        }
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "p95 Latency (ms)",
      "gridPos": { "x": 12, "y": 4, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "__PROM_DS__" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(gen_ai_client_operation_seconds_bucket[5m])) by (le)) * 1000",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        }
      }
    }
  ]
}
